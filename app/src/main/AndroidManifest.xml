<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.com.riskcontrol">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RiskControl">
        <activity
            android:name=".SettingUpMinimizationMeasure"
            android:exported="true" />
        <activity android:name=".SettingUpRiskTypeActivity" />
        <activity android:name=".SettingUpRiskActivity" />
        <activity android:name=".SettingUpFactoryActivity" />
        <activity android:name=".CurrentRegistryActivity" />
        <activity android:name=".SettingUpRegistryActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>